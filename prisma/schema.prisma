    name      String
    id        Int        @id @default(autoincrement())
model University {
model kierunek {
  id            Int           @id @default(autoincrement())
  name          String
  tryb          kierunek_tryb
  techniczne    Int
  spoleczne     Int
  humanistyczne Int
  artystyczne   Int
  sciscle       Int
  przyrodnicze  Int
  medyczne      Int
  rolnicze      Int
  createdAt     DateTime      @default(now())
  updatedAt     DateTime
  universityId  Int?
  university    university?   @relation(fields: [universityId], references: [id], map: "Kierunek_universityId_fkey")

  @@index([name], map: "Kierunek_name_idx")
  @@index([universityId], map: "Kierunek_universityId_fkey")
}

model university {
  id        Int        @id @default(autoincrement())
  name      String
  ranking   Float
  x         Float
  y         Float
  publiczny Boolean
  createdAt DateTime   @default(now())
  updatedAt DateTime
  miasto    String
  kierunek  kierunek[]

  @@index([name], map: "University_name_idx")
}

enum kierunek_tryb {
  stacjonarne
  niestacjonarne
  online
}
